<?xml version="1.0" encoding="UTF-8"?>
<widget id="at.wifiwien.wienwou" version="0.1.0-b019" xmlns="http://www.w3.org/ns/widget" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>SheetHero</name>
  <description>Learn Sheet music! Zack Zack Zack</description>
  <author email="manuel.czapek@gmx.net" href="http://www.wifiwien.at">
    Team Manuel
  </author>
  <content src="index.html" />
  <preferences name="android-minSdkVersion" value="28" />
  <preference name="Orientation" value="landscape" />

  <!-- fullscreenmods für Appgefühl -->
  <preference name="Fullscreen" value="true" />
  <plugin name="cordova-plugin-console" spec="~1.1.0" source="npm" />
  <plugin name="cordova-plugin-file" spec="~6.0.1" source="npm" />
  <plugin name="cordova-plugin-media" spec="~5.0.2" source="npm" />


  <platform name="android">
    <icon density="ldpi" src="www/assets/images/resizedmozart.png" />
  </platform>

</widget>
