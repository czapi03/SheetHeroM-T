<?xml version='1.0' encoding='utf-8'?>
<widget id="at.wifiwien.wienwou" version="0.1.1" xmlns="http://www.w3.org/ns/widget" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>SheetHero</name>
  <description>Learn Sheet music! Zack Zack Zack</description>
  <author email="manuel.czapek@gmx.net" href="http://www.wifiwien.at">
    Team Manuel
  </author>
  <content src="index.html"/>
  <preferences name="android-minSdkVersion" value="28"/>

  <preference name="Orientation" value="landscape"/>
  <preference name="Fullscreen" value="true"/>
  <preference name="phonegap-version" value="cli-8.0.0" />

  <plugin name="cordova-plugin-file" spec="~6.0.1" source="npm"/>
  <plugin name="cordova-plugin-media" spec="~5.0.2" source="npm"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.4" source="npm"/>
  <plugin name="cordova-plugin-insomnia" version="4.1.0" source="npm" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />


  <platform name="android">
    <icon density="ldpi" src="www/assets/images/resizedmozart.png"/>
  </platform>
</widget>
